   "source": [
    "# ML7_kNN\n",
    "# データ作成\n",
    "from sklearn.datasets import make_gaussian_quantiles\n",
    "ax, ay = make_gaussian_quantiles(n_samples=1000, n_features=2, random_state=0)\n",
    "# モデル構築（kNN）\n",
    "from sklearn.neighbors import KNeighborsClassifier\n",
    "m = KNeighborsClassifier()\n",
    "# 学習\n",
    "m.fit(ax, ay)\n",
    "# 精度算出\n",
    "m.score(ax, ay)"
   ]
  }
 ],
